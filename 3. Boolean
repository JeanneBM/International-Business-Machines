public class Operatory {
   public static void main(String[] args){
       int a = 6;
       int b = 5;

       boolean wartosc = a==b;
       System.out.println(wartosc);
   }
}
public class Operatory {
   public static void main(String[] args){
       int a = 6;
       int b = 5;

       boolean wartosc = a==b && a<b;
       System.out.println(wartosc);
   }
}
\\boolean wartosc = !(a==b);<<negacja
